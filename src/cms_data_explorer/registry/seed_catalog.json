    "data_domain": "medicaid",
    "api_endpoint": "https://data.medicaid.gov/resource/e5ds-i36p.json",
    "columns": [
      {"name": "state", "description": "State abbreviation", "data_type": "text", "example": "CA"},
      {"name": "labeler_code", "description": "First 5 digits of NDC", "data_type": "text"},
      {"name": "product_code", "description": "Middle 4 digits of NDC", "data_type": "text"},
      {"name": "package_size", "description": "Last 2 digits of NDC", "data_type": "text"},
      {"name": "ndc", "description": "National Drug Code", "data_type": "text"},
      {"name": "product_name", "description": "Drug product name", "data_type": "text", "example": "HYDROCODONE-ACETAMINOPHEN"},
