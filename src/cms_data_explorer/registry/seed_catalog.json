      {"name": "quarter", "description": "Quarter (1-4)", "data_type": "text"},
      {"name": "suppression_used", "description": "Whether data was suppressed", "data_type": "text"},
      {"name": "units_reimbursed", "description": "Number of units reimbursed", "data_type": "number"},
