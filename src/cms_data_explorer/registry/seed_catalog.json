[
  {
    "id": "xubh-q36u",
    "title": "Hospital General Information",
    "description": "General information about all Medicare-certified hospitals, including addresses, phone numbers, hospital type, and overall quality ratings.",
    "domain": "data.medicare.gov",
    "platform": "soda",
    "data_domain": "hospital_compare",
    "api_endpoint": "https://data.medicare.gov/resource/xubh-q36u.json",
    "columns": [
      {"name": "facility_id", "description": "CMS Certification Number (CCN)", "data_type": "text", "example": "010001"},
      {"name": "facility_name", "description": "Hospital name", "data_type": "text", "example": "SOUTHEAST HEALTH MEDICAL CENTER"},
      {"name": "address", "description": "Street address", "data_type": "text"},
      {"name": "city", "description": "City", "data_type": "text"},
      {"name": "state", "description": "State abbreviation", "data_type": "text", "example": "AL"},
      {"name": "zip_code", "description": "ZIP code", "data_type": "text"},
      {"name": "county_name", "description": "County", "data_type": "text"},
      {"name": "phone_number", "description": "Phone", "data_type": "text"},
      {"name": "hospital_type", "description": "Type of hospital", "data_type": "text", "example": "Acute Care Hospitals"},
      {"name": "hospital_ownership", "description": "Ownership type", "data_type": "text", "example": "Government - Hospital District or Authority"},
      {"name": "emergency_services", "description": "Has emergency services", "data_type": "text", "example": "Yes"},
      {"name": "hospital_overall_rating", "description": "Overall quality star rating (1-5)", "data_type": "text", "example": "3"}
    ],
    "keywords": ["hospital", "ratings", "quality", "star rating", "compare", "facility"],
    "temporal": "Updated quarterly",
    "join_keys": ["facility_id"],
    "notes": "Primary hospital dataset. facility_id is the CMS Certification Number (CCN). Ratings are 1-5 stars."
  },
  {
    "id": "4pq5-n9py",
    "title": "Nursing Home Provider Information",
    "description": "General information about Medicare and Medicaid certified nursing homes, including quality ratings, staffing, penalties, and ownership.",
    "domain": "data.medicare.gov",
    "platform": "soda",
    "data_domain": "nursing_home",
    "api_endpoint": "https://data.medicare.gov/resource/4pq5-n9py.json",
    "columns": [
      {"name": "federal_provider_number", "description": "CMS Certification Number", "data_type": "text", "example": "015001"},
      {"name": "provider_name", "description": "Nursing home name", "data_type": "text"},
      {"name": "provider_address", "description": "Street address", "data_type": "text"},
      {"name": "provider_city", "description": "City", "data_type": "text"},
      {"name": "provider_state", "description": "State abbreviation", "data_type": "text", "example": "AL"},
      {"name": "provider_zip_code", "description": "ZIP code", "data_type": "text"},
      {"name": "overall_rating", "description": "Overall quality star rating (1-5)", "data_type": "text", "example": "3"},
      {"name": "health_inspection_rating", "description": "Health inspection rating (1-5)", "data_type": "text"},
      {"name": "staffing_rating", "description": "Staffing rating (1-5)", "data_type": "text"},
      {"name": "rn_staffing_rating", "description": "RN staffing rating (1-5)", "data_type": "text"},
      {"name": "reported_nurse_aide_staffing_hours_per_resident_per_day", "description": "Nurse aide staffing hours per resident per day", "data_type": "text"},
      {"name": "number_of_certified_beds", "description": "Number of certified beds", "data_type": "text"},
      {"name": "average_number_of_residents_per_day", "description": "Average daily residents", "data_type": "text"},
      {"name": "provider_resides_in_hospital", "description": "Whether facility is in a hospital", "data_type": "text"}
