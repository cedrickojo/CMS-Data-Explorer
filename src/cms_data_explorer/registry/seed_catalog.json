  },
  {
    "id": "9n3s-kdb3",
    "title": "Timely and Effective Care - Hospital",
    "description": "Timely and effective care measures for hospitals, including ER wait times, heart attack care, and preventive care measures.",
    "domain": "data.medicare.gov",
    "platform": "soda",
    "data_domain": "quality_measures",
    "api_endpoint": "https://data.medicare.gov/resource/9n3s-kdb3.json",
    "columns": [
      {"name": "facility_id", "description": "CMS Certification Number (CCN)", "data_type": "text"},
      {"name": "facility_name", "description": "Hospital name", "data_type": "text"},
      {"name": "state", "description": "State abbreviation", "data_type": "text"},
      {"name": "condition", "description": "Condition or procedure category", "data_type": "text", "example": "Emergency Department"},
      {"name": "measure_id", "description": "CMS measure identifier", "data_type": "text", "example": "ED_2b"},
      {"name": "measure_name", "description": "Human-readable measure description", "data_type": "text"},
      {"name": "score", "description": "Measure score/value", "data_type": "text"},
      {"name": "sample", "description": "Sample size", "data_type": "text"}
    ],
    "keywords": ["quality", "measures", "ED wait time", "emergency", "heart attack", "timely care", "effective care"],
    "temporal": "Updated quarterly",
    "join_keys": ["facility_id"],
    "notes": "Join with Hospital General Information on facility_id for combined quality + general info analysis."
  },
  {
    "id": "nrth-mfg3",
    "title": "Medicare Spending Per Beneficiary - Hospital",
    "description": "Medicare spending per beneficiary at each hospital, including spending during episodes of care.",
    "domain": "data.medicare.gov",
    "platform": "soda",
    "data_domain": "spending",
    "api_endpoint": "https://data.medicare.gov/resource/nrth-mfg3.json",
    "columns": [
      {"name": "facility_id", "description": "CMS Certification Number (CCN)", "data_type": "text"},
      {"name": "facility_name", "description": "Hospital name", "data_type": "text"},
      {"name": "state", "description": "State abbreviation", "data_type": "text"},
      {"name": "measure_id", "description": "Spending measure ID", "data_type": "text", "example": "MSPB_1"},
      {"name": "score", "description": "Medicare spending per beneficiary score", "data_type": "text"}
    ],
    "keywords": ["spending", "cost", "Medicare spending", "per beneficiary", "MSPB", "episode"],
    "temporal": "Updated annually",
    "join_keys": ["facility_id"],
    "notes": "MSPB measures spending across an episode of care (3 days prior through 30 days post-discharge). Join on facility_id with Hospital General Information."
  },
  {
    "id": "yq43-i98g",
    "title": "Unplanned Hospital Visits - Hospital",
    "description": "Hospital readmission and return rates, including 30-day readmission measures for various conditions.",
    "domain": "data.medicare.gov",
    "platform": "soda",
    "data_domain": "hospital_readmissions",
    "api_endpoint": "https://data.medicare.gov/resource/yq43-i98g.json",
    "columns": [
      {"name": "facility_id", "description": "CMS Certification Number (CCN)", "data_type": "text"},
      {"name": "facility_name", "description": "Hospital name", "data_type": "text"},
      {"name": "state", "description": "State abbreviation", "data_type": "text"},
      {"name": "measure_id", "description": "Readmission measure ID", "data_type": "text", "example": "READM_30_HF"},
      {"name": "measure_name", "description": "Human-readable measure name", "data_type": "text"},
      {"name": "score", "description": "Readmission/return rate", "data_type": "text"},
      {"name": "denominator", "description": "Number of eligible patients", "data_type": "text"}
    ],
    "keywords": ["readmission", "return", "unplanned visit", "30-day", "heart failure", "pneumonia", "hip knee"],
    "temporal": "Updated quarterly",
    "join_keys": ["facility_id"],
    "notes": "Includes 30-day readmission rates for AMI, HF, pneumonia, COPD, hip/knee, CABG. Join on facility_id."
  },
  {
    "id": "npi_registry",
    "title": "NPI Registry",
    "description": "National Plan and Provider Enumeration System (NPPES) - look up healthcare providers by NPI number, name, specialty, or location.",
    "domain": "npiregistry.cms.hhs.gov",
    "platform": "npi",
    "data_domain": "npi_registry",
    "api_endpoint": "https://npiregistry.cms.hhs.gov/api/",
    "columns": [
      {"name": "number", "description": "10-digit NPI number", "data_type": "text", "example": "1234567890"},
      {"name": "basic.first_name", "description": "Provider first name (individual)", "data_type": "text"},
      {"name": "basic.last_name", "description": "Provider last name (individual)", "data_type": "text"},
      {"name": "basic.organization_name", "description": "Organization name (Type 2 NPI)", "data_type": "text"},
      {"name": "basic.credential", "description": "Provider credentials", "data_type": "text", "example": "M.D."},
      {"name": "basic.sex", "description": "Provider sex", "data_type": "text"},
      {"name": "basic.enumeration_date", "description": "Date NPI was assigned", "data_type": "date"},
      {"name": "basic.status", "description": "NPI status (A=Active)", "data_type": "text", "example": "A"},
      {"name": "addresses[].state", "description": "Practice location state", "data_type": "text"},
      {"name": "addresses[].city", "description": "Practice location city", "data_type": "text"},
      {"name": "taxonomies[].desc", "description": "Provider specialty/taxonomy", "data_type": "text", "example": "Internal Medicine"}
    ],
    "keywords": ["NPI", "provider", "physician", "doctor", "nurse", "specialty", "taxonomy", "lookup"],
    "join_keys": ["npi"],
    "notes": "Universal provider lookup. Max 200 results per query. Search by NPI number, name, state, city, or taxonomy/specialty. NPI links to most other CMS datasets."
  },
  {
    "id": "e5ds-i36p",
